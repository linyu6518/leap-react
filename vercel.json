{
  "version": 2,
  "buildCommand": "cd leap-angular && npm install && npm run build",
  "outputDirectory": "leap-angular/dist",
  "installCommand": "cd leap-angular && npm install",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
